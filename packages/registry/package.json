    "react-hook-form": "^7.49.3",
    "sonner": "^1.3.1"
  },
  "devDependencies": {
    "@energizeai/eslint-config": "*",
    "@energizeai/typescript-config": "*",
    "@turbo/gen": "^1.11.3",
    "@types/eslint": "^8.56.1",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "encoding": "^0.1.13",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  }
