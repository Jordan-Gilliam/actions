{
  "name": "actions",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "ade:dev": "turbo dev --filter ade",
    "ade:typecheck": "turbo run typecheck --filter ade",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "turbo run typecheck",
    "format:write": "turbo run format:write",
    "format:check": "turbo run format:check",
    "check": "turbo lint typecheck format:check",
    "generate:action": "turbo gen action-starter",
    "postinstall": "manypkg check",
    "publish-packages": "turbo run build lint && changeset version && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@energizeai/eslint-config": "*",
    "@energizeai/typescript-config": "*",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@manypkg/cli": "latest",
    "@trpc/react-query": "^10.45.0",
    "@typescript-eslint/parser": "^6.19.1",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.11.3",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "turbo": "latest",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.3",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
  ]
}
